{
	"name": "rover_interview_project",
	"version": "0.3.0",
	"description": "An example project for my interview with Rover.com in 2018.",
	"main": "./dist/server/server.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/scottbishopdev/RoverInterviewProject.git"
	},
	"author": "Scott Bishop",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/scottbishopdev/RoverInterviewProject/issues"
	},
	"homepage": "https://github.com/scottbishopdev/RoverInterviewProject#readme",
	"keywords": [
		"rover",
		"react",
		"typescript",
		"typegoose",
		"webpack",
		"mongodb",
		"docker",
		"openapi"
	],
	"scripts": {
		"clean": "npx rimraf \"package-lock.json\" \"./dist/*\" \"node_modules\" \"./*.tsbuildinfo\" && npx mkdirp dist && npm install",
		"clean:dist": "npx rimraf \"dist\"",
		"cleanAndBuild": "npm run clean && npm run build",
		"build:check": "npx tsc -p .",
		"build:server": "npx tsc -p ./tsconfig.server.json && npx copyfiles -u 1 ./src/api-definition.yaml dist",
		"build:client": "npx webpack --config webpack.config.js",
		"build": "npm run build:check && npm run build:server && npm run build:client",
		"start:server": "npx ts-node-dev --respawn dist/server/server.js",
		"start:client": "npx webpack-dev-server -d --content-base ./dist",
		"start": "npx concurrently \"npm run start:server\" \"npm run start:client\""
	},
	"dependencies": {
		"@typegoose/typegoose": "^7.6.0",
		"body-parser": "^1.19.0",
		"chalk": "^4.1.0",
		"commander": "^7.2.0",
		"concurrently": "^6.1.0",
		"csv": "^5.3.2",
		"express": "^4.17.1",
		"mongoose": "^5.12.9",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"swagger-ui-express": "^4.1.4",
		"yamljs": "^0.3.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.11",
		"@types/mongoose": "^5.10.5",
		"@types/node": "^15.3.0",
		"@types/react": "^17.0.5",
		"@types/react-dom": "^17.0.5",
		"@types/swagger-ui-express": "^4.1.2",
		"@types/webpack": "^5.28.0",
		"@types/yamljs": "^0.2.31",
		"clean-webpack-plugin": "^4.0.0-alpha.0",
		"html-webpack-plugin": "^5.3.1",
		"ts-loader": "^9.1.2",
		"ts-node-dev": "^1.1.6",
		"typescript": "^4.0.2",
		"webpack": "^5.37.0",
		"webpack-cli": "^4.7.0",
		"webpack-dev-server": "^3.11.2"
	}
}
